<%= button_to "Create an Image", new_image_path, method: :get %>
<%= button_to "View all Images", images_path, method: :get %>

<table>
  <% @images.each do |image| %>
    <tr>
      <td><%= image_tag(image.url, width: '280' ) %></td>
      <td><%= link_to 'show', image_path(image) %></td>
    </tr>
  <% end %>
</table>

